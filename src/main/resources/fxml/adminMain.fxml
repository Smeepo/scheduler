<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane fx:id="adminMainBorderPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="571.0" prefWidth="801.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.patricklass.scheduler.control.AdminMainController">
   <left>
      <TableView fx:id="adminUserTableView" maxHeight="333.0" prefHeight="333.0" prefWidth="300.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="adminUserColumn" prefWidth="299.0" text="Nutzer" />
        </columns>
      </TableView>
   </left>
   <right>
      <TableView fx:id="adminGroupTableView" maxHeight="334.0" prefHeight="333.0" prefWidth="300.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="adminGroupColumn" prefWidth="309.0" text="Gruppen" />
        </columns>
      </TableView>
   </right>
   <center>
      <Button fx:id="addUserButton" mnemonicParsing="false" text="Hinzufügen" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <HBox>
         <children>
            <VBox>
               <children>
                  <Button fx:id="delUserButton" mnemonicParsing="false" prefWidth="280.0" text="Nutzer löschen">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="toggleAdminButton" mnemonicParsing="false" prefWidth="280.0" text="Admin toggle" />
               </children>
            </VBox>
            <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <VBox>
               <children>
                  <Button fx:id="delGroupButton" mnemonicParsing="false" prefWidth="280.0" text="Gruppe löschen">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="createGroupButton" alignment="CENTER" mnemonicParsing="false" prefWidth="280.0" text="Neue Gruppe" />
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </HBox>
   </bottom>
   <top>
      <VBox alignment="CENTER_RIGHT" prefHeight="80.0" prefWidth="801.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnLogout" mnemonicParsing="false" onAction="#logout" text="Logout">
               <VBox.margin>
                  <Insets right="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </top>
</BorderPane>
